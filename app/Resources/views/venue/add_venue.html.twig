{% extends 'base.html.twig' %}

{%  block title %}
    New Venue
{% endblock %}

{% block body %}
    <h1>Add a venue</h1>
    {% form_theme form 'bootstrap_3_horizontal_layout.html.twig' %}

    {{ form_start(form) }}
    {{ form_row(form.name) }}
    {{ form_row(form.adress) }}
    {{ form_row(form.city) }}
    {{ form_row(form.postalCode) }}
    {{ form_row(form.description) }}
    {{ form_end(form) }}
{% endblock %}